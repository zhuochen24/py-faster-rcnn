train_net: "models/pascal_voc/VGG16/faster_rcnn_end2end/tmp_train_val/train_all_round2.prototxt"
base_lr: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 50000
display: 200
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
iter_size: 2
snapshot_prefix: "vgg16_lininterp_finetune2_5"
