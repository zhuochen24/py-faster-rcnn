train_net: "models/pascal_voc/ZF/faster_rcnn_end2end/tmp_train_val/train_all.prototxt"

base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 50000
display: 200
average_loss: 100
momentum: 0.9
weight_decay: 0.0005


# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
iter_size: 2
snapshot_prefix: "zf_lininterp_origin"
